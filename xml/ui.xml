<?xml version="1.0" encoding="UTF-8"?>
<!-- 根元素 -->
<Universal_UI>
  <!-- 通用模块 -->
  <Universal_Module>
    <Root_Dir_Path></Root_Dir_Path>
    <Pictures_Dir_Path>pictures</Pictures_Dir_Path>
  </Universal_Module>
  <!-- 字休模块 -->
  <Font_Module>
    <Title_Font></Title_Font>
  </Font_Module>
  <!-- 主窗口模块 -->
  <MainWindow_Module>
    <!-- 无边框 -->
    <Rimless>yes</Rimless>
    <Menubar>no</Menubar>
    <Statusbar>no</Statusbar>
    <MinWidth>1200</MinWidth>
    <MinHeight>900</MinHeight>
    <Background_Color>background: white</Background_Color>
  </MainWindow_Module>
  <!-- 标题模块 -->
  <Title_Module>
    <Picture_name>title</Picture_name>
    <Min_WidgetWidth>900</Min_WidgetWidth>
    <Min_WidgetHeight>40</Min_WidgetHeight>
    <Title_Text>High-speed rail picture detection system</Title_Text>
  </Title_Module>
  <!-- 标题栏最小化、最大化、关闭标签模块 -->
  <TitleLabel_Module>
    <!-- 鼠标跟踪(是否为窗口小部件启用了鼠标跟踪) -->
    <MouseTracking>yes</MouseTracking>
    <!-- 最小化 -->
    <Min_PictureDefault>mindefault</Min_PictureDefault>
    <Min_PictureMoved>minmoved</Min_PictureMoved>
    <Min_WidgetMinWidth>100</Min_WidgetMinWidth>
    <Min_WidgetMinHeight>40</Min_WidgetMinHeight>
    <!-- 最大化 -->
    <!-- 窗口正常显示时 -->
    <Normal_PictureDefault>normaldefault</Normal_PictureDefault>
    <Normal_PictureMoved>normalmoved</Normal_PictureMoved>
    <Normal_WidgetMinWidth>100</Normal_WidgetMinWidth>
    <Normal_WidgetMinHeight>40</Normal_WidgetMinHeight>
    <!-- 窗口最大化显示时 -->
    <Max_PictureDefault>maxdefault</Max_PictureDefault>
    <Max_PictureMoved>maxmoved</Max_PictureMoved>
    <Max_WidgetMinWidth>100</Max_WidgetMinWidth>
    <Max_WidgetMinHeight>40</Max_WidgetMinHeight>
    <!-- 关闭 -->
    <Shut_PictureDefault>shutdefault</Shut_PictureDefault>
    <Shut_PictureMoved>shutmoved</Shut_PictureMoved>
    <Shut_WidgetMinWidth>100</Shut_WidgetMinWidth>
    <Shut_WidgetMinHeight>40</Shut_WidgetMinHeight>
  </TitleLabel_Module>
  <!-- 门脸模块（标题栏下方展示图片的区域）-->
  <Doorface_Module>
    <Picture_name>doorface</Picture_name>
    <Picture_Width>300</Picture_Width>
    <Picture_Height>400</Picture_Height>
  </Doorface_Module>
  <!-- 菜单模块 -->
  <Menus_Module>
    <!-- 菜单一 -->
    <Menu>
      <Object_Name>menu1</Object_Name>
      <Picture_name>menu1</Picture_name>
      <Picture_Width>10</Picture_Width>
      <Picture_Height>20</Picture_Height>
      <Submenu>
        <!--
        <Picture_name>submenu1</Picture_name>
        <Picture_Width>30</Picture_Width>
        <Picture_Height>40</Picture_Height>
        -->
        <Object_Name>menu1</Object_Name>
        <Hide>yes</Hide>
        <Item1_Name objectName="quadScrren" clickedPicture="clickedmenu1submenu1item1">menu1submenu1item1</Item1_Name>
        <Item2_Name objectName="" clickedPicture="clickedmenu1submenu1item2">menu1submenu1item2</Item2_Name>
        <Item3_Name objectName="" clickedPicture="clickedmenu1submenu1item3">menu1submenu1item3</Item3_Name>
        <End></End>
      </Submenu>
    </Menu>
    <!-- 菜单二 -->
    <Menu>
      <Object_Name>menu2</Object_Name>
      <Picture_name>menu2</Picture_name>
      <Picture_Width>50</Picture_Width>
      <Picture_Height>60</Picture_Height>
      <Submenu>
        <!--
        <Picture_name>submenu2</Picture_name>
        <Picture_Width>70</Picture_Width>
        <Picture_Height>80</Picture_Height>
        -->
        <Object_Name>menu2</Object_Name>
        <Hide>yes</Hide>
        <Item1_Name objectName="" clickedPicture="clickedmenu2submenu1item1">menu2submenu1item1</Item1_Name>
        <Item2_Name objectName="testWidget" clickedPicture="clickedmenu2submenu1item2">menu2submenu1item2</Item2_Name>
        <Item3_Name objectName="" clickedPicture="clickedmenu2submenu1item3">menu2submenu1item3</Item3_Name>
        <Item1_Name objectName="" clickedPicture="clickedmenu2submenu1item4">menu2submenu1item4</Item1_Name>
        <Item2_Name objectName="" clickedPicture="clickedmenu2submenu1item5">menu2submenu1item5</Item2_Name>
        <Item3_Name objectName="" clickedPicture="clickedmenu2submenu1item6">menu2submenu1item6</Item3_Name>
        <End></End>
      </Submenu>
    </Menu>
    <!-- 菜单三 -->
    <Menu>
      <Object_Name>menu3</Object_Name>
      <Picture_name>menu3</Picture_name>
      <Picture_Width>90</Picture_Width>
      <Picture_Height>95.8</Picture_Height>
      <Submenu>
        <!--
        <Picture_name>submenu3</Picture_name>
        <Picture_Width>20.8</Picture_Width>
        <Picture_Height>2.89</Picture_Height>
        -->
        <Object_Name>menu3</Object_Name>
        <Hide>yes</Hide>
        <Item1_Name objectName="splitScreenDisplay32Channel" clickedPicture="clickedmenu3submenu1item1">menu3submenu1item1</Item1_Name>
        <Item2_Name objectName="" clickedPicture="clickedmenu3submenu1item2">menu3submenu1item2</Item2_Name>
        <Item3_Name objectName="" clickedPicture="clickedmenu3submenu1item3">menu3submenu1item3</Item3_Name>
        <End></End>
      </Submenu>
    </Menu>
  </Menus_Module>
  <!-- 展示模块 -->
  <Display_Module>
    <Width>800</Width>
    <Height>900</Height>
  </Display_Module>
  <!-- 布局模块 -->
  <Layout>
    <!-- 分隔符 -->
    <Separator>,</Separator>
    <!-- 主窗口布局 -->
    <!-- 左、上、右、下 写入时不应该包含空格，只应该包含上面指定的分隔符 -->
    <MainWindow_ContentsMargins>11,0,11,11</MainWindow_ContentsMargins>
    <MainWindow_Spacing>0</MainWindow_Spacing>
    <!-- 以下调整的都是布局在主窗口布局中的表现(下面这些布局全是主窗口中的小布局) -->
    <!-- 标题栏布局 -->
    <Titlebar_ContentsMargins>0,0,0,0</Titlebar_ContentsMargins>
    <Titlebar_Spacing></Titlebar_Spacing>
    <Titlebar_Stretch>2</Titlebar_Stretch>
    <!-- 门脸布局 -->
    <Doorface_ContentsMargins>0,0,0,0</Doorface_ContentsMargins>
    <Doorface_Spacing></Doorface_Spacing>
    <Doorface_Stretch>4</Doorface_Stretch>
    <!-- 菜单栏和内容区(这是一个水平布局, 包含菜单栏和内容区，调整的是整个水平布局在父布局中的表现) -->
    <MenuContent_ContentsMargins>0,11,0,0</MenuContent_ContentsMargins>
    <MenuContent_Spacing></MenuContent_Spacing>
    <MenuContent_Stretch>50</MenuContent_Stretch>
    <!-- 以下调整的是具体的小部件在加入到具体小布局中的表现 -->
    <!-- 菜单栏 -->
    <Menubar_Stretch>0</Menubar_Stretch>
    <Menubar_Alignment></Menubar_Alignment>
    <!-- 内容区 -->
    <Content_Stretch>5</Content_Stretch>
    <Content_Alignment></Content_Alignment>

  </Layout>
</Universal_UI>
