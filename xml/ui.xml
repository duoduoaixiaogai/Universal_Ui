<?xml version="1.0" encoding="UTF-8"?>
<!-- 根元素 -->
<Universal_UI>
  <!-- 通用模块 -->
  <Universal_Module>
    <Root_Dir_Path></Root_Dir_Path>
    <Pictures_Dir_Path>pictures</Pictures_Dir_Path>
  </Universal_Module>
  <!-- 字休模块 -->
  <Font_Module>
    <Title_Font></Title_Font>
  </Font_Module>
  <!-- 主窗口模块 -->
  <MainWindow_Module>
    <!-- 无边框 -->
    <Rimless>yes</Rimless>
    <Menubar>no</Menubar>
    <Statusbar>no</Statusbar>
    <MinWidth>1200</MinWidth>
    <MinHeight>900</MinHeight>
  </MainWindow_Module>
  <!-- 标题模块 -->
  <Title_Module>
    <Picture_name>title.jpg</Picture_name>
    <Picture_Width>100</Picture_Width>
    <Picture_Height>200</Picture_Height>
    <Title_Text>High-speed rail picture detection system</Title_Text>
  </Title_Module>
  <!-- 标题栏最小化、最大化、关闭标签模块 -->
  <TitleLabel_Module>
    <!-- 鼠标跟踪(是否为窗口小部件启用了鼠标跟踪) -->
    <MouseTracking>yes</MouseTracking>
    <!-- 最小化 -->
    <Min_PictureDefault></Min_PictureDefault>
    <Min_PictureMoved></Min_PictureMoved>
    <Min_WidgetMinWidth></Min_WidgetMinWidth>
    <Min_WidgetMinHeight></Min_WidgetMinHeight>
    <!-- 最大化 -->
    <!-- 窗口正常显示时 -->
    <Normal_PictureDefault></Normal_PictureDefault>
    <Normal_PictureMoved></Normal_PictureMoved>
    <Normal_WidgetMinWidth></Normal_WidgetMinWidth>
    <Normal_WidgetMinHeight></Normal_WidgetMinHeight>
    <!-- 窗口最大化显示时 -->
    <Max_PictureDefault></Max_PictureDefault>
    <Max_PictureMoved></Max_PictureMoved>
    <Max_WidgetMinWidth></Max_WidgetMinWidth>
    <Max_WidgetMinHeight></Max_WidgetMinHeight>
    <!-- 关闭 -->
    <Shut_PictureDefault></Shut_PictureDefault>
    <Shut_PictureMoved></Shut_PictureMoved>
    <Shut_WidgetMinWidth></Shut_WidgetMinWidth>
    <Shut_WidgetMinHeight></Shut_WidgetMinHeight>
  </TitleLabel_Module>
  <!-- 门脸模块（标题栏下方展示图片的区域）-->
  <Doorface_Module>
    <Picture_name>doorface.jpg</Picture_name>
    <Picture_Width>300</Picture_Width>
    <Picture_Height>400</Picture_Height>
  </Doorface_Module>
  <!-- 菜单模块 -->
  <Menus_Module>
    <!-- 菜单一 -->
    <Menu>
      <Picture_name>menu1</Picture_name>
      <Picture_Width>10</Picture_Width>
      <Picture_Height>20</Picture_Height>
      <Submenu>
        <Picture_name>submenu1</Picture_name>
        <Picture_Width>30</Picture_Width>
        <Picture_Height>40</Picture_Height>
      </Submenu>
    </Menu>
    <!-- 菜单二 -->
    <Menu>
      <Picture_name>menu2</Picture_name>
      <Picture_Width>50</Picture_Width>
      <Picture_Height>60</Picture_Height>
      <Submenu>
        <Picture_name>submenu2</Picture_name>
        <Picture_Width>70</Picture_Width>
        <Picture_Height>80</Picture_Height>
      </Submenu>
    </Menu>
    <!-- 菜单三 -->
    <Menu>
      <Picture_name>menu3</Picture_name>
      <Picture_Width>90</Picture_Width>
      <Picture_Height>95.8</Picture_Height>
      <Submenu>
        <Picture_name>submenu3</Picture_name>
        <Picture_Width>20.8</Picture_Width>
        <Picture_Height>2.89</Picture_Height>
      </Submenu>
    </Menu>
  </Menus_Module>
  <!-- 展示模块 -->
  <Display_Module>
    <Width>800</Width>
    <Height>900</Height>
  </Display_Module>
</Universal_UI>
